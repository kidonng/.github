{
  "diffEditor.renderSideBySide": false,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.find.autoFindInSelection": "multiline",
  "editor.fontFamily": "JetBrains Mono, JetBrainsMono Nerd Font, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.lineNumbers": "relative",
  "editor.minimap.enabled": false,
  "editor.renderIndentGuides": false,
  "editor.smoothScrolling": true,
  "extensions.ignoreRecommendations": true,
  "files.autoGuessEncoding": true,
  "files.autoSave": "onFocusChange",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "formattingToggle.affects": ["formatOnPaste", "formatOnSave"],
  "git.showPushSuccessNotification": true,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "npm.scriptExplorerAction": "run",
  "oneDarkPro.bold": true,
  "oneDarkPro.editorTheme": "oneDarkProFlat",
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "search.smartCase": true,
  "tabnine.experimentalAutoImports": true,
  "telemetry.enableTelemetry": false,
  "terminal.external.osxExec": "kitty.app",
  "terminal.integrated.enableBell": true,
  "terminal.integrated.experimentalUseTitleEvent": true,
  "terminal.integrated.macOptionIsMeta": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "vetur.experimental.templateInterpolationService": true,
  "vetur.useWorkspaceDependencies": true,
  "vim.hlsearch": true,
  "workbench.colorTheme": "One Dark Pro",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.smoothScrolling": true,
  "workbench.tree.renderIndentGuides": "none"
}
